{
  "name": "desktop-for-web3",
  "version": "0.1.0",
  "description": "iOS-style new tab page for Web3 enthusiasts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "node tests/run-automated-test.cjs",
    "test:quick": "node tests/run-automated-test.cjs --quick",
    "test:ui": "node tests/run-automated-test.cjs",
